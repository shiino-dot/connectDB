{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "inputDocument",
      "type": "cosmosDB",
      "databaseName": "qandaDB",
      "collectionName": "questions",
      "id": "{id}",
      "connection": "documentDB",
      "direction": "in"
  },
  {
      "name": "outputDocument",
      "type": "cosmosDB",
      "databaseName": "qandaDB",
      "collectionName": "questions",
      "connection": "documentDB",
      "direction": "out"
  }
  ]
}
