{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "post/"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "outputDocument",
      "type": "cosmosDB",
      "databaseName": "qandaDB",
      "collectionName": "answers",
      "createIfNotExists": true,
      "connectionStringSetting": "cosmosdbConnectionStr",
      "direction": "out",
      "partitionKey": "/id"
    },
    {
      "name": "outputquestions",
      "type": "cosmosDB",
      "databaseName": "qandaDB",
      "collectionName": "questions",
      "createIfNotExists": true,
      "connectionStringSetting": "cosmosdbConnectionStr",
      "direction": "out",
      "partitionKey": "/id"
    }
  ],
  "disabled": false
}